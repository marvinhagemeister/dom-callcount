{
  "name": "dom-callcount",
  "version": "1.1.1",
  "description": "Track callcount of dom apis. Useful for vdom-testing.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/dom-callcount.git"
  },
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "scripts": {
    "cleanup": "rimraf dist/",
    "lint": "tslint 'src/**/*.ts' 'src/**/*.tsx'",
    "watch": "tsc -w",
    "build": "npm run cleanup && tsc -p tsconfig.build.json",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepublish": "npm run lint && npm t && npm run build",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/jsdom": "^11.0.1",
    "husky": "^0.14.2",
    "jest": "^21.2.1",
    "lint-staged": "^4.0.0",
    "prettier": "^1.9.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.1.0",
    "tslint": "^5.1.0",
    "tslint-config-sevenval": "^0.6.3",
    "typescript": "^2.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --trailing-comma=all --write",
      "npm run lint",
      "git add"
    ]
  }
}
